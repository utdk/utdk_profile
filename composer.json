{
  "name": "utexas/utdk_profile",
  "description": "UTDK Drupal 8.x installation profile",
  "type": "drupal-profile",
  "keywords": [
    "Drupal",
    "UTexas"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "UT Austin ITS Campus Solutions â€“ Web Content Management Solutions Team",
      "email": "drupal-kit-support@utlists.utexas.edu"
    }
  ],
  "support": {
    "email": "drupal-kit-support@utlists.utexas.edu",
    "issues": "https://github.austin.utexas.edu/eis1-wcs/utdk_profile/issues",
    "source": "https://github.austin.utexas.edu/eis1-wcs/utdk_profile"
  },
  "require": {
    "composer/installers": "1.7.0",
    "cweagans/composer-patches": "1.6",
    "drupal/admin_toolbar": "1.25",
    "drupal/adminimal_admin_toolbar": "1.8",
    "drupal/adminimal_theme": "1.3",
    "drupal/breadcrumbs_visibility": "1.0",
    "drupal/ckeditorheight": "1.3",
    "drupal/core-recommended": "8.8.0",
    "drupal/date_ap_style": "1.0.0-rc1",
    "drupal/editor_advanced_link": "1.4",
    "drupal/features": "3.8",
    "drupal/feed_block": "1.0",
    "drupal/google_cse": "3.0",
    "drupal/google_tag": "1.1",
    "drupal/iframe_title_filter": "1.0",
    "drupal/layout_builder_modal": "1.0-alpha2",
    "drupal/layout_builder_restrictions": "2.1",
    "drupal/layout_builder_styles": "1.0-beta1",
    "drupal/linkit": "5.0-beta8",
    "drupal/media_library_theme_reset": "1.0-beta1",
    "drupal/metatag": "1.8",
    "drupal/menu_block": "1.5",
    "drupal/page_title_visibility": "1.1",
    "drupal/pathauto": "1.6-beta1",
    "drupal/pathologic": "1.0-alpha1",
    "drupal/redirect": "1.3",
    "drupal/responsive_tables_filter": "1.2",
    "drupal/svg_upload_sanitizer": "1.0.0-alpha1",
    "drush/drush": "~8",
    "oomphinc/composer-installers-extender": "1.1.2",
    "rvtraveller/qs-composer-installer": "1.1",
    "utexas/forty_acres": "3.0.0-beta.1",
    "utexas/utexas_qualtrics_filter": "1.0.0-beta.1",
    "zaporylie/composer-drupal-optimizations": "1.0.2"
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-name": "utexas",
    "drupal-scaffold": {
      "file-mapping": {
        "[web-root]/sites/default/settings.php": {
          "path": "assets/example.settings.php",
          "overwrite": false
        },
        "[web-root]/android-chrome-192x192.png": {
          "path": "assets/android-chrome-192x192.png",
          "overwrite": false
        },
        "[web-root]/android-chrome-512x512.png": {
          "path": "assets/android-chrome-512x512.png",
          "overwrite": false
        },
        "[web-root]/apple-touch-icon.png": {
          "path": "assets/apple-touch-icon.png",
          "overwrite": false
        },
        "[web-root]/browserconfig.xml": {
          "path": "assets/browserconfig.xml",
          "overwrite": false
        },
        "[project-root]/.gitignore": {
          "path": "assets/example.gitignore",
          "overwrite": false
        },
        "[web-root]/favicon.ico": {
          "path": "assets/favicon.ico",
          "overwrite": false
        },
        "[web-root]/mstile-150x150.png": {
          "path": "assets/mstile-150x150.png",
          "overwrite": false
        },
        "[web-root]/safari-pinned-tab.svg": {
          "path": "assets/safari-pinned-tab.svg",
          "overwrite": false
        },
        "[web-root]/site.webmanifest": {
          "path": "assets/site.webmanifest",
          "overwrite": false
        }
      }
    }
  }
}
