{
  "name": "utexas/utdk_profile",
  "type": "drupal-custom-profile",
  "description": "Drupal 9 installation profile for the UT Drupal Kit",
  "keywords": [
    "Drupal",
    "UT Drupal Kit"
  ],
  "license": [
    "GPL-2.0-or-later"
  ],
  "authors": [
    {
      "name": "Web Content Management Solutions Team, Information Technology Services, The University of Texas at Austin",
      "email": "drupal-kit-support@utlists.utexas.edu",
      "homepage": "https://drupalkit.its.utexas.edu",
      "role": "Developer"
    }
  ],
  "require": {
    "cweagans/composer-patches": "~1.0",
    "drupal/addtoany": "1.16.0",
    "drupal/admin_toolbar": "2.5.0",
    "drupal/breadcrumbs_visibility": "1.2.0",
    "drupal/ckeditorheight": "1.8.0",
    "drupal/core-composer-scaffold": "9.3.19",
    "drupal/core-recommended": "9.3.19",
    "drupal/date_ap_style": "1.5.0",
    "drupal/diff": "1.0.0",
    "drupal/editor_advanced_link": "2.0.0",
    "drupal/entity_clone": "1.0-beta6",
    "drupal/features": "3.12.0",
    "drupal/feed_block": "1.1.0",
    "drupal/google_cse": "3.7.0",
    "drupal/google_tag": "1.5.0",
    "drupal/iframe_title_filter": "1.1.0",
    "drupal/layout_builder_modal": "1.1.0",
    "drupal/layout_builder_restrictions": "2.13.0",
    "drupal/layout_builder_styles": "1.0.0-beta4",
    "drupal/linkit": "6.0.0-beta3",
    "drupal/media_library_form_element": "2.0.3",
    "drupal/media_library_theme_reset": "1.1.0",
    "drupal/menu_block": "1.8.0",
    "drupal/metatag": "1.21.0",
    "drupal/page_title_visibility": "1.3.0",
    "drupal/pantheon_advanced_page_cache": "1.2.0",
    "drupal/pathauto": "1.10.0",
    "drupal/pathologic": "1.0.0-alpha3",
    "drupal/redirect": "1.7.0",
    "drupal/responsive_tables_filter": "1.13.0",
    "drupal/role_delegation": "1.2.0",
    "drupal/seckit": "2.0.0",
    "drupal/schema_metatag": "2.3.0",
    "drupal/svg_upload_sanitizer": "1.2.0",
    "drupal/twitter_profile_widget": "3.0.2",
    "drupal/updated": "1.0.0-alpha1",
    "drupal/url_embed": "1.0-beta1",
    "drupal/xmlsitemap": "1.2.0",
    "utexas/forty_acres": "3.7.0",
    "npm-asset/accessible360--accessible-slick": "1.0.1",
    "oomphinc/composer-installers-extender": "2.0.1",
    "utexas/utexas_qualtrics_filter": "3.0.0"
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "extra": {
    "patches": {
      "drupal/core": {
        "Fix issue with CKEditor image align center": "https://drupalkit.its.utexas.edu/patches/1480-ckeditor-align.patch",
        "Fix minimum MariaDb version for Pantheon": "https://drupalkit.its.utexas.edu/patches/1494-mariadb-version.patch",
        "Impossible to update inline blocks after reverting to a previous revision": "https://drupalkit.its.utexas.edu/patches/1122-layout-builder-revert.patch",
        "Improve performance of functional tests by caching Drupal installations": "https://www.drupal.org/files/issues/2021-02-16/2900208-33.patch",
        "Media Library: Default value for data-align attribute should not be center": "https://drupalkit.its.utexas.edu/patches/1559-media-library-video-width.patch"
      },
      "drupal/entity_clone": {
        "Add a way to exclude certain entity types from cloning": "https://drupalkit.its.utexas.edu/patches/1709-entity-clone-hook.patch"
      },
      "drupal/linkit": {
        "LinkIt-style autocomplete affects multiple adjacent link fields": "https://drupalkit.its.utexas.edu/patches/1106-linkit-multiple-autocomplete.patch",
        "Form button obscured if Layout Builder is being used": "https://drupalkit.its.utexas.edu/patches/1723-linkit-relative-modal.patch"
      },
      "drupal/media_library_form_element": {
        "Media referenced via the Media Library Form Element, when deleted, cause an AJAX error that prevents editing": "https://drupalkit.its.utexas.edu/patches/1842-media-library-form-element-null.patch"
      },
      "drupal/url_embed": {
        "Add option to make embeds responsive": "https://drupalkit.its.utexas.edu/patches/1225-url-embed-responsive.patch"
      }
    },
    "drupal-scaffold": {
      "file-mapping": {
        "[web-root]/robots.txt": {
          "path": "assets/robots.txt"
        },
        "[web-root]/android-chrome-192x192.png": {
          "path": "assets/android-chrome-192x192.png",
          "overwrite": false
        },
        "[web-root]/android-chrome-512x512.png": {
          "path": "assets/android-chrome-512x512.png",
          "overwrite": false
        },
        "[web-root]/apple-touch-icon.png": {
          "path": "assets/apple-touch-icon.png",
          "overwrite": false
        },
        "[web-root]/browserconfig.xml": {
          "path": "assets/browserconfig.xml",
          "overwrite": false
        },
        "[web-root]/favicon.ico": {
          "path": "assets/favicon.ico",
          "overwrite": false
        },
        "[web-root]/favicon-32x32.png": {
          "path": "assets/favicon-32x32.png",
          "overwrite": false
        },
        "[web-root]/favicon-48x48.png": {
          "path": "assets/favicon-48x48.png",
          "overwrite": false
        },
        "[web-root]/mstile-150x150.png": {
          "path": "assets/mstile-150x150.png",
          "overwrite": false
        },
        "[web-root]/safari-pinned-tab.svg": {
          "path": "assets/safari-pinned-tab.svg",
          "overwrite": false
        },
        "[web-root]/site.webmanifest": {
          "path": "assets/site.webmanifest",
          "overwrite": false
        }
      }
    },
    "installer-name": "utexas"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "support": {
    "email": "drupal-kit-support@utlists.utexas.edu",
    "docs": "https://drupalkit.its.utexas.edu/docs"
  }
}
