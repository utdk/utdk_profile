{
  "name": "utexas/utdk_profile",
  "type": "drupal-custom-profile",
  "description": "Drupal 9 installation profile for the UT Drupal Kit",
  "keywords": [
    "Drupal",
    "UT Drupal Kit"
  ],
  "license": [
    "GPL-2.0-or-later"
  ],
  "authors": [
    {
      "name": "Web Content Management Solutions Team, Information Technology Services, The University of Texas at Austin",
      "email": "drupal-kit-support@utlists.utexas.edu",
      "homepage": "https://drupalkit.its.utexas.edu",
      "role": "Developer"
    }
  ],
  "require": {
    "cweagans/composer-patches": "~1.0",
    "drupal/addtoany": "2.0.1",
    "drupal/admin_toolbar": "3.3.2",
    "drupal/breadcrumbs_visibility": "1.3.0",
    "drupal/config_update": "2.0.0-alpha2",
    "drupal/core-composer-scaffold": "10.0.9",
    "drupal/core-recommended": "10.0.9",
    "drupal/date_ap_style": "1.5.0",
    "drupal/diff": "1.1.0",
    "drupal/editor_advanced_link": "2.1.1",
    "drupal/entity_clone": "2.0.0-beta2",
    "drupal/features": "5.0.0-alpha1",
    "drupal/feed_block": "2.0.0",
    "drupal/google_cse": "3.8.0",
    "drupal/google_tag": "1.6.0",
    "drupal/iframe_title_filter": "2.0.0",
    "drupal/layout_builder_iframe_modal": "1.3.0",
    "drupal/layout_builder_restrictions": "2.17.0",
    "drupal/layout_builder_styles": "2.0",
    "drupal/linkit": "6.0.0-rc1",
    "drupal/media_library_form_element": "2.0.6",
    "drupal/menu_block": "1.10.0",
    "drupal/metatag": "1.22.0",
    "drupal/page_title_visibility": "1.6.0",
    "drupal/pantheon_advanced_page_cache": "2.1.1",
    "drupal/pathauto": "1.11.0",
    "drupal/pathologic": "2.0.0-alpha1",
    "drupal/redirect": "1.8.0",
    "drupal/responsive_tables_filter": "1.17.0",
    "drupal/role_delegation": "1.2.0",
    "drupal/seckit": "2.0.1",
    "drupal/schema_metatag": "2.4.0",
    "drupal/svg_upload_sanitizer": "1.4.0",
    "drupal/twitter_profile_widget": "3.1.3",
    "drupal/updated": "1.1.0",
    "drupal/url_embed": "2.0.0-alpha1",
    "drupal/xmlsitemap": "1.4.0",
    "utexas/forty_acres": "3.11.0",
    "npm-asset/accessible360--accessible-slick": "1.0.1",
    "oomphinc/composer-installers-extender": "2.0.1",
    "utexas/utexas_qualtrics_filter": "3.1.0"
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "extra": {
    "patches": {
      "drupal/core": {
        "Improve performance of functional tests by caching Drupal installations": "https://www.drupal.org/files/issues/2021-02-16/2900208-33.patch",
        "Respect row height configuration in CKEditor 5 + Drupal 9.5.x": "https://drupalkit.its.utexas.edu/patches/2145-ckeditor-rows-d10.patch"
      },
      "drupal/entity_clone": {
        "Add a way to exclude certain entity types from cloning": "https://drupalkit.its.utexas.edu/patches/1709b-entity-clone-updated.patch",
        "Inline Blocks on cloned nodes are deleted when originating nodes are deleted": "https://drupalkit.its.utexas.edu/patches/2046-entityclone-inline-block.patch"
      },
      "drupal/url_embed": {
        "CKEditor 5 Support": "https://drupalkit.its.utexas.edu/patches/2093-ckeditor5-support-url-embed.patch",
        "CKEditor 5 Support: Responsive embeds": "https://drupalkit.its.utexas.edu/patches/2093-ckeditor5-support-url-embed-responsive.patch"
      },
      "drupal/xmlsitemap": {
        "Call to undefined method getImplementations": "https://drupalkit.its.utexas.edu/patches/2193-xmlsitemap-3323148.patch"
      }
    },
    "drupal-scaffold": {
      "file-mapping": {
        "[web-root]/robots.txt": {
          "path": "assets/robots.txt"
        },
        "[web-root]/android-chrome-192x192.png": {
          "path": "assets/android-chrome-192x192.png",
          "overwrite": false
        },
        "[web-root]/android-chrome-512x512.png": {
          "path": "assets/android-chrome-512x512.png",
          "overwrite": false
        },
        "[web-root]/apple-touch-icon.png": {
          "path": "assets/apple-touch-icon.png",
          "overwrite": false
        },
        "[web-root]/browserconfig.xml": {
          "path": "assets/browserconfig.xml",
          "overwrite": false
        },
        "[web-root]/favicon.ico": {
          "path": "assets/favicon.ico",
          "overwrite": false
        },
        "[web-root]/favicon-32x32.png": {
          "path": "assets/favicon-32x32.png",
          "overwrite": false
        },
        "[web-root]/favicon-48x48.png": {
          "path": "assets/favicon-48x48.png",
          "overwrite": false
        },
        "[web-root]/mstile-150x150.png": {
          "path": "assets/mstile-150x150.png",
          "overwrite": false
        },
        "[web-root]/safari-pinned-tab.svg": {
          "path": "assets/safari-pinned-tab.svg",
          "overwrite": false
        },
        "[web-root]/site.webmanifest": {
          "path": "assets/site.webmanifest",
          "overwrite": false
        }
      }
    },
    "installer-name": "utexas"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "support": {
    "email": "drupal-kit-support@utlists.utexas.edu",
    "docs": "https://drupalkit.its.utexas.edu/docs"
  }
}
