{
  "name": "utexas/utdk8",
  "description": "UTDK Drupal 8.x CMS",
  "type": "project",
  "keywords": ["Drupal", "UTexas"],
  "license": "MIT",
  "authors": [
    {
      "name": "Mark Fullmer",
      "email": "jfullmer@austin.utexas.edu",
      "homepage": "https://markfullmer.com",
      "role": "Developer"
    },
    {
      "name": "Tyler Fahey",
      "email": "tfahey@austin.utexas.edu",
      "homepage": "https://tylerfahey.com",
      "role": "Developer"
    },
    {
      "name": "Michael Marsello",
      "email": "mmarsello@austin.utexas.edu",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "drupal-kit-support@utlists.utexas.edu",
    "issues": "https://github.austin.utexas.edu/eis1-wcs/utdk8-shared/issues",
    "source": "https://github.austin.utexas.edu/eis1-wcs/utdk8-shared"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "vcs",
      "url":  "git@github.austin.utexas.edu:eis1-wcs/forty_acres.git"
    }
  ],
  "require": {
    "composer/installers": "1.5.0",
    "cweagans/composer-patches": "1.6",
    "drupal-composer/drupal-scaffold": "2.0.1",
    "drupal/admin_toolbar":"1.20",
    "drupal/adminimal_admin_toolbar":"1.5",
    "drupal/adminimal_theme":"1.3",
    "drupal/console": "1.2.0",
    "drupal/core": "8.6.0",
    "drupal/features": "3.5",
    "drupal/layout_builder_restrictions": "1.2",
    "drupal/paragraphs": "1.3",
    "drupal/pathauto": "1.2",
    "drush/drush": "~8",
    "utexas/forty_acres": "dev-master",
    "rvtraveller/qs-composer-installer": "1.1",
    "oomphinc/composer-installers-extender": "1.1.2",
    "webflo/drupal-core-strict": "8.6.0",
    "drupal/date_ap_style": "1.x-dev"
  },
  "require-dev": {
    "mikey179/vfsstream": "^1.2",
    "behat/behat": "3.*",
    "behat/mink": "^1.7",
    "behat/mink-extension": "^2.2",
    "behat/mink-goutte-driver": "^1.2",
    "jcalderonzumba/gastonjs": "^1.0.2",
    "jcalderonzumba/mink-phantomjs-driver": "^0.3.1",
    "drupal/drupal-extension": "^3.1",
    "drush-ops/behat-drush-endpoint": "^0.0.4",
    "phpunit/phpunit": "^4.8.35 || ^6.5",
    "phpspec/prophecy": "^1.4",
    "symfony/css-selector": "^3.4.0",
    "symfony/phpunit-bridge": "^3.4.3",
    "symfony/debug": "^3.4.0"
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "classmap": [
      "scripts/composer/ScriptHandler.php"
    ]
  },
  "scripts": {
    "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
    "build-assets": [
      "composer install --optimize-autoloader"
    ],
    "post-install-cmd": [
      "@drupal-scaffold",
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ],
    "post-update-cmd": [
      "@drupal-scaffold",
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ],
    "test-utexas": [
      "php web/core/scripts/run-tests.sh --php /usr/local/bin/php PHPUnit --suppress-deprecations --dburl mysql://drupal8:drupal8@database/drupal8 --url http://utdk8.lndo.site --module utexas --concurrency 4 --verbose"
    ]
  },
  "config": {
    "process-timeout": 0
  },
  "extra": {
    "patches": {
      "drupal/features": {
        "Class declaration needs to be uppercased":
        "https://www.drupal.org/files/issues/2018-08-15/2992790-drupal-component-namespace-case.patch"
      }
    },
    "installer-types": ["utexas-module", "utexas-theme"],
    "installer-paths": {
      "web/core": ["type:drupal-core"],
      "web/profiles/utexas/modules/composer_managed/contrib/{$name}": [
        "drupal/admin_toolbar",
        "drupal/adminimal_admin_toolbar",
        "drupal/config_update",
        "drupal/ctools",
        "drupal/date_ap_style",
        "drupal/entity_reference_revisions",
        "drupal/features",
        "drupal/layout_builder_restrictions",
        "drupal/paragraphs",
        "drupal/pathauto",
        "drupal/token"
      ],
      "web/profiles/utexas/modules/composer_managed/custom/{$name}": ["type:utexas-module"],
      "web/profiles/utexas/themes/composer_managed/contrib/{$name}": [
        "drupal/adminimal_theme"
      ],
      "web/profiles/utexas/themes/composer_managed/custom/{$name}": [
        "utexas/forty_acres"
      ],
      "drush/contrib/{$name}": ["type:drupal-drush"]
    },
    "build-env": {
      "install-cms": [
        "drush site-install standard --account-mail={account-mail} --account-name={account-name} --account-pass={account-pass} --site-mail={site-mail} --site-name={site-name} --yes",
        "drush pm-enable config_direct_save --yes"
      ],
      "export-configuration": "drush config-export --yes"
    },
    "drupal-scaffold": {
      "excludes": [
        ".csslintrc",
        ".eslintrc",
        ".editorconfig",
        ".eslintignore",
        ".eslintrc.json",
        "web.config"
      ]
    }
  }
}
